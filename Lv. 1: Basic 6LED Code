


void setup() {

pinMode(3, OUTPUT);
pinMode(5, OUTPUT);
pinMode(6, OUTPUT);
pinMode(9, OUTPUT);
pinMode(10, OUTPUT);
pinMode(11, OUTPUT);

}

void loop() {
  int d = 1000;
  
      digitalWrite(3, HIGH);
      delay(d);
      digitalWrite(3, LOW);
      delay(d);

      digitalWrite(5, HIGH);
      delay(d);
      digitalWrite(5, LOW);
      delay(d);

      digitalWrite(6, HIGH);
      delay(d);
      digitalWrite(6, LOW);
      delay(d);

      digitalWrite(9, HIGH);
      delay(d);
      digitalWrite(9, LOW);
      delay(d);

      digitalWrite(10, HIGH);
      delay(d);
      digitalWrite(10, LOW);
      delay(d);

      digitalWrite(11, HIGH);
      delay(d);
      digitalWrite(11, LOW);
      delay(d);
}
}
